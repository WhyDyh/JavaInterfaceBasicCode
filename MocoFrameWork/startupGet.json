[
  {
    "description": "模拟一个没有参数的get请求",
    "request": {
      "uri": "/getdemo",
      "method": "get"
    },
    "response": {
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      },
      "text": "这是没有参数的get请求"
    }
  },
  {
    "description": "模拟一个带参数的get请求",
    "request": {
      "uri": "/getWithParam",
      "method": "get",
      "queries": {
        "name": "张三",
        "age": "18"
      }
    },
    "response": {
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      },
      "text": "张三回来了！！！"
    }
  }

]